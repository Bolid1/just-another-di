    "exclude": [
      "lib/**/*.spec.ts"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/assert": "^0.0.31",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.1",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.0.5",
    "typescript": "^2.9.1"
  }
}
